  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch udp.ts",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/src/udp.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch tcp.ts",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/src/tcp.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}